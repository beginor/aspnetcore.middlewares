{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "customHeader": {
    "headers": {
      "X-Frame-Options": "SAMEORIGIN",
      "X-Content-Type-Options": "nosniff",
      "X-XSS-Protection": "1; mode=block"
    }
  },
  "spaFailback": {
    "rules": [
      { "pathbase": "/handset/(?!assets/).*", "failback": "/handset/index.html" },
      { "pathbase": "/tablet/(?!assets/).*", "failback": "/tablet/index.html" },
      { "pathbase": "/web/(?!assets/).*", "failback": "/web/index.html" }
    ]
  }
}
